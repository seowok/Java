			left_list.add(line);
		}		
		
		right_list = new ArrayList<Line>();
		for(int i = 0; i < 6; i ++){
			Line line = new NotComparedLine("num " + i);
			left_list.add(line);
		}		
	}
	
	@Test
	public void testMatchEqualLine() {
		compare_line.constructLine(text_pane)
	}

